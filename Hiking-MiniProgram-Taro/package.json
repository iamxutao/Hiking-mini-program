    "@tarojs/runtime": "^4.1.9",
    "@tarojs/taro": "^4.1.9",
    "dayjs": "^1.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
